package com.haorenao.app.bean.th;


/**
 * 茶友之家
 * 首页新闻新闻实体类
 */
public class BigPicture extends THEntity {
	
	private String id;
	private String pic_url;
	private String link;
	

	public String getLink() {
		return link;
	}

	public void setLink(String link) {
		this.link = link;
	}

	public BigPicture(){
		
	}

	public String getId() {
		return id;
	}


	public void setId(String id) {
		this.id = id;
	}


	public String getPic_url() {
		return pic_url;
	}


	public void setPic_url(String pic_url) {
		this.pic_url = pic_url;
	}

	@Override
	public String toString() {
		return "BigPicture [id=" + id + ", pic_url=" + pic_url + ", link="
				+ link + "]";
	}



}
